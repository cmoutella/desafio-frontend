{
  "name": "QConcursos_Challenge",
  "version": "1.0.0",
  "description": "desafio tecnico",
  "main": "./dist/index.js",
  "author": "Carolina Moutella < ola@carolinamoutella.com >",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel server watch",
    "server": "browser-sync start --server . --files assets",
    "watch": "npm-watch",
    "postcss:dev": "postcss assets/css --replace --use autoprefixer --env devepment",
    "css:dev": "node-sass --source-map-embed --output-style expanded assets/css src/css",
    "js:dev": "webpack --mode development"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "autoprefixer": "^9.4.2",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.3",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.5.0",
    "postcss": "^7.0.6",
    "postcss-cli": "^6.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "watch": {
    "css:dev": {
      "patterns": "src/css/**",
      "extensions": "sass",
      "quiet": true
    },
    "js:dev": {
      "patterns": "src/js/**",
      "extensions": "js",
      "quiet": true
    }
  },
  "browserslist": {
    "development": [
      "last 2 major versions"
    ]
  }
}
